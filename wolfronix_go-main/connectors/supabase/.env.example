# Supabase Connector Configuration
# Copy to .env and fill in your values

# Supabase project URL (found in Settings → API)
SUPABASE_URL=https://your-project.supabase.co

# Supabase service_role key (NOT the anon key — found in Settings → API → service_role)
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIs...

# API key that Wolfronix engine will send in X-Wolfronix-API-Key header
# Must match CLIENT_DB_API_KEY in Wolfronix engine config
CONNECTOR_API_KEY=your-connector-api-key

# Port for this connector server
PORT=8080
