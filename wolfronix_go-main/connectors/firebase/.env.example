# Firebase Connector Configuration
# Copy to .env and fill in your values

# Path to your Firebase service account JSON file
# Download from: Firebase Console → Project Settings → Service Accounts → Generate New Private Key
FIREBASE_SERVICE_ACCOUNT=./serviceAccountKey.json

# Firebase Storage bucket (for large encrypted files)
# Found in Firebase Console → Storage → gs://your-bucket.appspot.com
FIREBASE_STORAGE_BUCKET=your-project.appspot.com

# API key that Wolfronix engine will send in X-Wolfronix-API-Key header
CONNECTOR_API_KEY=your-connector-api-key

# Port for this connector server
PORT=8080
