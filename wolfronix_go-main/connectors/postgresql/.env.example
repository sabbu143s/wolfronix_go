# PostgreSQL Connector Configuration
# Copy to .env and fill in your values

# PostgreSQL connection string
# Local:   postgresql://wolfronix:password@localhost:5432/wolfronix
# Remote:  postgresql://user:pass@db.example.com:5432/wolfronix?sslmode=require
POSTGRES_URI=postgresql://wolfronix:password@localhost:5432/wolfronix

# API key that Wolfronix engine will send in X-Wolfronix-API-Key header
CONNECTOR_API_KEY=your-connector-api-key

# Port for this connector server
PORT=8080
